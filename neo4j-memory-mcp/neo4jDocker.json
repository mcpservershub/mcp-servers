{
  "mcpServers": {
    "neo4j-memory": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "--network",
        "neo4j",
        "-e",
        "NEO4J_URL=bolt://neo4j-memory;7687",
        "-e",
        "NEO4J_USERNAME=neo4j",
        "-e",
        "NEO4J_PASSWORD=iamtesting",
        "santoshkal/neo4j-memory:test"
      ]
    }
  }
}
